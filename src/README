clive -- C audio live-coding skeleton

deps:
  jack-dev
  bash
  make
  gcc
  git
  ecasound
  htop
  nano
  xterm
  inotifywait (inotify-tools)

usage:
  git clone .../clive.git
  cd clive/src && ./start.sh
  edit go.c code, save -> automatic git commit + recompile + reload

visuals deps:
  pkg-config
  libv4l-dev
  libjpeg-dev
  libglew-dev
  libglfw3-dev
  libglm-dev
